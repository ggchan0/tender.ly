<html>
   <% include ../../partials/header %>
   </head>
   <body>
      EDIT GROUPS
      <ul>
      <!-- Loops through and populated each group from gropData (sent in /edit get) -->
      <% groupData.forEach(function(el, index) {%>
         <li><a href='/groups/group/<%= el["name"];%>'><%= el["name"];%></a></li>
      <%});%>
      </ul>
      </br>
      Add New Group
      <!-- post to /groups/add with the form -->
      <form action="/groups/add" method="post">
      Group Name
      <input type="text" name="groupName" placeholder="Group Name" />
      <br>
      <button type="submit">Submit</button>
      </form>
   <% include ../../partials/footer %>
   </body>
</html>
